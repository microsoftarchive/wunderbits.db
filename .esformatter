{
  "//REFERENCE": "https://github.com/millermedeiros/esformatter/blob/master/lib/preset/default.json",
  "root": true,
  "preset" : "default",
  "indent" : {
    "value" : "  "
  },
  "lineBreak" : {
    "value" : "\n",
    "keepEmptyLines" : 1,
    "before": {
      "FunctionDeclaration": 1,
      "ElseStatement": 1,
      "ElseIfStatement": 1,
      "VariableName": 0,
      "VariableDeclaration": 0,
      "VariableDeclarationWithoutInit": 0
    },
    "after": {
      "FunctionExpression": 1,
      "FunctionDeclaration": 1,
      "VariableDeclaration": 0
    }
  },
  "whiteSpace" : {
    "before": {
      "FunctionExpressionOpeningBrace": 1
    },
    "after": {
      "FunctionReservedWord": 1,
      "FunctionName" : 1,
      "FunctionExpressionClosingBrace" : 1
    }
  }
}